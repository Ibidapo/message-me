<div class="main">
  <%= render 'layouts/logo'%>

  <%= form_for :session, url: "/login", method: 'post' do |f| %>
    <%= f.text_field :username, placeholder: "Username" %>
    <%= f.password_field :password, placeholder: "Password" %>
    <%= f.submit "Login" %>
  <% end %>

  <small>You don't have an account? <a href="#">Sign Up</a></small>
</div>